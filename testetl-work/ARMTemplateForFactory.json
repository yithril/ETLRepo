											"path": "$['ReportDateFrom']"
										},
										"sink": {
											"path": "$['AuthorizationCode']"
										}
									},
									{
										"source": {
											"path": "$['ReportDateTo']"
										},
										"sink": {
											"path": "$['AuthorizationAmount']"
										}
									},
									{
										"source": {
											"path": "$['ReportGenerated']"
										},
										"sink": {
											"path": "$['TransactionAmount']"
										}
									},
