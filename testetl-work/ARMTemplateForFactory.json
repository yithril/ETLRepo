		"etlpreprocess_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "etlpool"
		},
		"etlpreprocess_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "etlpreprocess"
		}
