									{
										"source": {
											"path": "$['TransactionRecordType']"
										},
										"sink": {
											"path": "$['Network']"
										}
									},
									{
										"source": {
											"path": "$['SubmissionDate']"
										},
										"sink": {
											"path": "$['CardPresentType']"
										}
									},
									{
										"source": {
											"path": "$['PID']"
										},
										"sink": {
											"path": "$['InterchangeFee']"
										}
									},
									{
										"source": {
											"path": "$['PIDShort']"
										},
										"sink": {
											"path": "$['NetworkFee']"
										}
									},
									{
										"source": {
											"path": "$['SubmissionNumber']"
										},
										"sink": {
											"path": "$['ProcessorFee']"
										}
